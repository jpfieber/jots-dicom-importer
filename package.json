{
  "name": "jots-dicom-importer",
  "version": "1.0.0",
  "description": "Import DICOM files into your vault.",
  "main": "dist/main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "version": "node version-bump.mjs && git add manifest.json versions.json"
  },
  "keywords": [
    "obsidian",
    "plugin",
    "dicom",
    "medical",
    "images"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "cornerstone-core": "^2.6.1",
    "cornerstone-wado-image-loader": "^4.13.2",
    "dicom-parser": "^1.8.21",
    "hammerjs": "^2.0.8"
  },
  "devDependencies": {
    "@types/cornerstone-core": "^2.3.0",
    "@types/hammerjs": "^2.0.41",
    "@types/node": "^16.11.6",
    "builtin-modules": "^5.0.0",
    "esbuild": "^0.14.0",
    "obsidian": "^1.1.1",
    "typescript": "^4.5.0"
  }
}